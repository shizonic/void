Xft.autohint:     0
Xft.lcdfilter:    lcddefault
! Xft.hintstyle:  hintfull
Xft.hintstyle:    hintslight
Xft.hinting:      1
Xft.antialias:    1
Xft.rgba:         rgb

*.font:             xft:Ubuntu Mono:size=12,style=Regular
*.boldFont:         xft:Ubuntu Mono:size=12,style=Bold
*.italicFont:       xft:Ubuntu Mono:size=12,style=Italic
*.boldItalicFont:   xft:Ubuntu Mono:size=12,style=Bold Italic
